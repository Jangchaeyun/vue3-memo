{"ast":null,"code":"import { reactive } from \"vue\";\nimport axios from 'axios';\nexport default {\n  setup() {\n    const state = reactive({\n      data: []\n    });\n\n    const add = () => {\n      const content = prompt(\"내용을 입력해주세요.\");\n\n      if (!content) {\n        return alert(\"내용을 입력해주세요\");\n      }\n\n      axios.post(\"/api/memos\", {\n        content\n      }).then(res => {\n        state.data = res.data;\n      });\n    };\n\n    const edit = id => {\n      const content = prompt(\"내용을 입력해주새요\", state.data[id]);\n      console.log(content);\n      axios.put(\"/api/memos/\" + idx, {\n        content\n      }).then(res => {\n        state.data = res.data;\n      });\n    };\n\n    axios.get(\"/api/memos\").then(res => {\n      state.data = res.data;\n    });\n    return {\n      state,\n      add,\n      edit\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAaA,SAAQA,QAAR,QAAuB,KAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACnBI,IAAI,EAAC;IADc,CAAD,CAAtB;;IAGA,MAAMC,GAAE,GAAI,MAAM;MACd,MAAMC,OAAM,GAAIC,MAAM,CAAC,aAAD,CAAtB;;MAEA,IAAG,CAACD,OAAJ,EAAa;QACT,OAAOE,KAAK,CAAC,YAAD,CAAZ;MACJ;;MACAP,KAAK,CAACQ,IAAN,CAAW,YAAX,EAAyB;QAAEH;MAAF,CAAzB,EAAsCI,IAAtC,CAA4CC,GAAD,IAAS;QAChDR,KAAK,CAACC,IAAN,GAAaO,GAAG,CAACP,IAAjB;MACH,CAFD;IAGH,CATD;;IAWA,MAAMQ,IAAG,GAAKC,EAAD,IAAQ;MACjB,MAAMP,OAAM,GAAIC,MAAM,CAAC,YAAD,EAAeJ,KAAK,CAACC,IAAN,CAAWS,EAAX,CAAf,CAAtB;MACAC,OAAO,CAACC,GAAR,CAAYT,OAAZ;MACAL,KAAK,CAACe,GAAN,CAAU,gBAAgBC,GAA1B,EAA+B;QAACX;MAAD,CAA/B,EAA0CI,IAA1C,CAAgDC,GAAD,IAAS;QACpDR,KAAK,CAACC,IAAN,GAAaO,GAAG,CAACP,IAAjB;MACH,CAFD;IAGJ,CANA;;IAQAH,KAAK,CAACiB,GAAN,CAAU,YAAV,EAAwBR,IAAxB,CAA8BC,GAAD,IAAS;MAClCR,KAAK,CAACC,IAAN,GAAaO,GAAG,CAACP,IAAjB;IACH,CAFD;IAIA,OAAO;MAAED,KAAF;MAASE,GAAT;MAAcO;IAAd,CAAP;EACH;;AA7BU,CAAf","names":["reactive","axios","setup","state","data","add","content","prompt","alert","post","then","res","edit","id","console","log","put","idx","get"],"sourceRoot":"","sources":["E:\\vue3-memo\\frontend\\src\\components\\Memo.vue"],"sourcesContent":["<template>\r\n    <div class=\"memo\">\r\n        <div class=\"act\">\r\n            <button class=\"btn btn-primary\" @click=\"add()\">+ 추가</button>\r\n        </div>\r\n        <ul>\r\n            <li v-for=\"(d, idx) in state.data\" :key=\"d.id\" @click=\"edit(idx)\">{{ d.content }}</li>\r\n\r\n        </ul>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive} from \"vue\";\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    setup() {\r\n        const state = reactive({\r\n            data:[],\r\n        })\r\n        const add = () => {\r\n            const content = prompt(\"내용을 입력해주세요.\");\r\n            \r\n            if(!content) {\r\n                return alert(\"내용을 입력해주세요\");\r\n            }\r\n            axios.post(\"/api/memos\", { content }).then((res) => {\r\n                state.data = res.data;\r\n            });\r\n        };\r\n\r\n        const edit = (id) => {\r\n            const content = prompt(\"내용을 입력해주새요\", state.data[id]);\r\n            console.log(content);\r\n            axios.put(\"/api/memos/\" + idx, {content}).then((res) => {\r\n                state.data = res.data;\r\n            });\r\n        }\r\n\r\n        axios.get(\"/api/memos\").then((res) => {\r\n            state.data = res.data;\r\n        });\r\n\r\n        return { state, add, edit };\r\n    },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.memo {\r\n    .act {\r\n        padding: 10px 10px 5px 5px;\r\n        text-align: right;\r\n    }\r\n\r\n    ul {\r\n        list-style: none;\r\n        padding: 15px;\r\n        margin: 0;\r\n\r\n    li {\r\n        padding: 15px;\r\n        margin: 10px 0;\r\n        border: 1px solid #eee;\r\n    }\r\n    }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}